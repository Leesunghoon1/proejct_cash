<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.easyfestival.www.repository.PackageDAO">
  
  <insert id="register">
  	insert into package(pk_name, pk_price) values(#{pkName}, #{pkPrice})
  </insert>
  
  <select id="getlist" resultType="com.easyfestival.www.domain.PackageVO">
  	select * from package;
  </select>
  
  <select id="getDetail" resultType="com.easyfestival.www.domain.PackageVO">
  	select * from package where pk_no = #{pkNo}
  </select>
  
  </mapper>